 <div class="container-fluid">
   <div class="row">
    <div class="wrap">
      
      <div id="tinderslide">
        <ul>
          <%= render 'pet' %>  
        </ul>
      </div>

      <div class="actions">
       <a href="#" class="dislike"><i class="fa fa-times"></i></a>
       <a href="#" class="like"><i class="fa fa-heart"></i></a>
     </div>
     
   </div>
   

 </div>

</div>

<!--EXCLUIR --> 
<div class="margin-div"></div>

<div class="container-fluid">
    <% @pets.each do |pet| %>

     <b>Nome:</b> <%= pet.name %>
     <b>Nome:</b> <%= pet.user.firstName %>
     <b>Nome:</b> <%= pet.name %>
     <b>Nome:</b> <%= pet.name %>
 
 
   <% end %>
   

</div>



<script type="text/javascript">
  $("#tinderslide > ul li").tsort({attr:"id"});

  $("#tinderslide").jTinder({
    onDislike: function (item) {
      alert('Dislike image ' + (item.index()+1));
    },
    onLike: function (item) {
      alert('Like image ' + (item.index()+1));
    },
    animationRevertSpeed: 200,
    animationSpeed: 400,
    threshold: 1,
    likeSelector: '.like',
    dislikeSelector: '.dislike'
  });
</script>
