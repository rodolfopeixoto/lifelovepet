		<tr>
			<th scope="row"><%= image_tag("#{match.image(:thumb)}"), class: "img-circle" %></th>
			<td><%= link_to match.name, profile_user_path(match.id) %></td>
			<td><%= match.updated_at.strftime("%d/%m/%Y") %></td>
			<td class="text-right">
				<a href="" data-toggle="tooltip" data-placement="top" title="Email"><i class="fa fa fa-envelope"></i></a> 
				<%= link_to destroy_friendships_path(friend_id: match.id), :data => { toggle: "tooltip", placement: "top", confirm: "Quer um unmatch com #{match.name}?" }, title: "Unmatch", id: "delete_friendship", method: :delete do %>
                    <i class="fa fa-times"></i>
				<% end %>
				<%= link_to matches_get_email_path(id: "#{match.id}", :data => { toggle: "tooltip", placement: "top" }, title: "Get Email", remote: :true do) %>
                    <i class="fa fa-weixin"></i>
			    <% end %>

			</td>
		</tr> 